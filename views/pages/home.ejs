  /* common image style */
  .insta-img {
    position: absolute;
    width: 75%;
    border-radius: 40px 0 40px 0;
    object-fit: cover;
    transition: all 0.4s ease;
  }

  /* center image (front) */
  .insta-center {
    z-index: 3;
    transform: scale(1);
    box-shadow: 0 25px 45px rgba(0, 0, 0, 0.3);
  }

  /* left image (behind) */
  .insta-left {
    z-index: 1;
    transform: translateX(-60px) rotate(-6deg) scale(0.92);
    opacity: 0.7;
  }

  /* right image (behind) */
  .insta-right {
    z-index: 2;
    transform: translateX(60px) rotate(6deg) scale(0.92);
    opacity: 0.7;
  }
  .insta-stack:hover .insta-left {
    transform: translateX(-80px) rotate(-8deg) scale(0.9);
  }

  .insta-stack:hover .insta-right {
    transform: translateX(80px) rotate(8deg) scale(0.9);
  }

  .insta-stack:hover .insta-center {
    transform: scale(1.03);
  }
  .speaker-card {
    height: 100%;
